spring:
  application:
    name: runnit-api

  datasource:
    url: jdbc:mysql://localhost:3306/runnit_db?createDatabaseIfNotExist=true&serverTimezone=UTC
    username: runnit_user
    password: runnit_pass
    driver-class-name: com.mysql.cj.jdbc.Driver

  jpa:
    hibernate:
      ddl-auto: validate
    show-sql: false
    properties:
      hibernate:
        dialect: org.hibernate.dialect.MySQLDialect
        format_sql: true

  flyway:
    enabled: true
    baseline-on-migrate: true
    locations: classpath:db/migration

jwt:
  secret: runnit-super-secret-key-please-change-in-production-minimum-256-bits
  expiration: 86400000

aws:
  s3:
    bucket-name: runnit-uploads-dev
    region: us-west-2
  access-key-id: your-access-key
  secret-access-key: your-secret-key

